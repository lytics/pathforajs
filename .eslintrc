{
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "jasmine": true
  },
  "rules": {
    "comma-dangle": 2,
    "no-cond-assign": 1,
    "no-console": 2,
    "no-constant-condition": 2,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 1,
    "no-empty": 2,
    "no-ex-assign": 2,
    "no-extra-parens": 1,
    "no-extra-semi": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-obj-calls": 2,
    "no-sparse-arrays": 2,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "use-isnan": 2,
    "valid-typeof": 2,
    "curly": 2,
    "eqeqeq": 2,
    "no-alert": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-lone-blocks": 2,
    "no-magic-numbers": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-new": 2,
    "no-param-reassign": 2,
    "no-redeclare": 1,
    "no-self-compare": 1,
    "no-useless-concat": 1,
    "radix": 2,
    "vars-on-top": 2,
    "yoda": [2, "never"],
    "no-delete-var": 2,
    "no-shadow": 2,
    "no-undef-init": 2,
    "no-undef": 1,
    "no-undefined": 2,
    "no-unused-vars": 2,
    "no-use-before-define": 2,
    "global-require": 2,
    "no-path-concat": 2,
    "array-bracket-spacing": [2, "never"],
    "block-spacing": [2, "always"],
    "brace-style": [2, "1tbs", { "allowSingleLine": false }],
    "camelcase": 2,
    "comma-style": [2, "last"],
    "computed-property-spacing": [2, "never"],
    "eol-last": 2,
    "indent": [2, 2],
    "max-len": [1, 80, 4],
    "no-lonely-if": 1,
    "no-multiple-empty-lines": 1,
    "no-mixed-spaces-and-tabs": 2,
    "no-trailing-spaces": 2,
    "semi": [2, "always"],
    "space-before-blocks": [2, "always"],
    "space-before-function-paren": [2, { "anonymous": "always", "named": "never" }],
    "space-before-keywords": [2, "always"],
    "space-in-parens": [2, "never"],
    "space-infix-ops": 2
  }
}